<odoo>
	
	<record id="hr_expense_form_brand" model="ir.ui.view">
		<field name="name">hr.expense.form.brand</field>
		<field name="model">hr.expense</field>
		<field name="inherit_id" ref="hr_expense.hr_expense_view_form"/>
		<field name="arch" type="xml">
			<field name="company_id" position="after">
				<field name="brand_id" context="{'search_brand_by_company': company_id}"/>
			</field>
		</field>
	</record>

</odoo>
